{
  "name": "wespals/ghostscript-console",
  "type": "library",
  "description": "A Symfony CLI utility for the Ghostscript executable",
  "keywords": ["ghostscript", "pdf", "cli", "symfony"],
  "license": "MIT",
  "authors": [
    {
      "name": "Wes Pals",
      "email": "wespals@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/console": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "GhostscriptConsole\\": "src/GhostscriptConsole/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\GhostscriptConsole\\": "tests/GhostscriptConsole/"
    }
  },
  "bin": [
    "bin/ghostscript-console"
  ]
}
